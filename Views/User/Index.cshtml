@{
    ViewBag.Title = "User";
    ViewBag.Logo = "User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Contet Header -->
<section class="content-header">
    <h1 class="card-text">
    Users
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">User Activity</li>
    </ol>
</section>
<!-- Main Content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box card-custom">
                <div class="box-header">
                    <h2 class="box-title"><b>User Activity</b></h2>
                </div>
                <div class="box-body">
                    <div class="dataTables_wrapper form-inline dt-bootstrap" id="example1_wrapper">
                        <div id="tbl-loading" class=""></div>
                        <div class="row">
                            <div class="col-sm-12 table-responsive">
                                <table id="tblUser" class="table table-bordered table-striped table-hover">
                                    <thead class="bg-me">
                                        <tr>
                                            <th>UserName<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Kategori<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Login<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Logout<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Status<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section scripts {
    <script src="~/Scripts/User.js" type="text/javascript"></script>
}