@{
    ViewBag.Title = "Mahasiswa";
    ViewBag.Logo = "Mahasiswa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Contet Header -->
<section class="content-header">
    <h1>
    Mahasiswa
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Mahasiswa</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box card-custom">
                <div class="box-header">
                    <h2 class="box-title"><b>Mahasiswa List</b></h2>
                </div>
                <div class="box-body">
                    <div>
                        <label>
                            <button class="btn btn-sm btn-success" id="btn-addmhspage"><strong>Add Mahasiswa</strong></button>
                        </label>
                    </div>
                    <div class="dataTables_wrapper form-inline dt-bootstrap" id="example1_wrapper">
                        <div id="spinner"></div>
                        <div class="row">
                            <div class="col-sm-12 table-responsive">
                                <table id="tblMahasiswa" class="table table-bordered table-striped table-hover">
                                    <thead class="bg-me">
                                        <tr>
                                            <th>Nama<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Fakultas<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>NIM<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Kelas<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Email<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Status<i class="pull-right glyphicon glyphicon-sort-by-attributes"></i></th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section scripts {
    <script src="~/Scripts/Mahasiswa.js" type="text/javascript"></script>
}